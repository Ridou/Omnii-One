### 1.3 AuthContext Unit Tests

**File**: `tests/unit/auth/authContext.test.ts`

```typescript
import { renderHook, act } from '@testing-library/react-native';
import { AuthProvider, useAuth } from '~/context/AuthContext';
import { signInWithApple } from '~/lib/auth/appleAuth';
import { signInWithGoogle } from '~/lib/auth/googleAuth';

jest.mock('~/lib/auth/appleAuth');
jest.mock('~/lib/auth/googleAuth');
jest.mock('~/lib/supabase');

describe('AuthContext with Apple Sign In', () => {
  const wrapper = ({ children }: { children: React.ReactNode }) => (
    <AuthProvider>{children}</AuthProvider>
  );

  it('should add Apple Sign In method to context', () => {
    const { result } = renderHook(() => useAuth(), { wrapper });

    expect(result.current).toHaveProperty('signInWithApple');
    expect(typeof result.current.signInWithApple).toBe('function');
  });

  it('should handle Apple Sign In success', async () => {
    // Arrange
    const mockAuthData = {
      user: { id: 'user-123', email: 'test@privaterelay.appleid.com' },
      session: { access_token: 'apple-token' }
    };
    
    jest.mocked(signInWithApple).mockResolvedValue(mockAuthData);
    
    const { result } = renderHook(() => useAuth(), { wrapper });

    // Act
    await act(async () => {
      await result.current.signInWithApple();
    });

    // Assert
    expect(signInWithApple).toHaveBeenCalled();
    expect(result.current.isLoading).toBe(false);
  });

  it('should handle Apple Sign In failure', async () => {
    // Arrange
    jest.mocked(signInWithApple).mockRejectedValue(new Error('Apple auth failed'));
    
    const { result } = renderHook(() => useAuth(), { wrapper });

    // Act & Assert
    await act(async () => {
      await expect(result.current.signInWithApple()).rejects.toThrow('Apple auth failed');
    });
    
    expect(result.current.isLoading).toBe(false);
  });

  it('should maintain Google Sign In functionality', async () => {
    // Arrange
    const mockGoogleAuth = {
      user: { id: 'user-456', email: 'test@gmail.com' },
      session: { access_token: 'google-token' }
    };
    
    jest.mocked(signInWithGoogle).mockResolvedValue(mockGoogleAuth);
    
    const { result } = renderHook(() => useAuth(), { wrapper });

    // Act
    await act(async () => {
      await result.current.signInWithGoogle();
    });

    // Assert
    expect(signInWithGoogle).toHaveBeenCalled();
    expect(result.current.isLoading).toBe(false);
  });
});
