### 1.1 Apple Authentication Unit Tests

**File**: `tests/unit/auth/appleAuth.test.ts`

```typescript
import { describe, it, expect, beforeEach } from 'bun:test';
import { mock } from 'bun:test';
import { signInWithApple } from '~/lib/auth/appleAuth';
import * as AppleAuthentication from 'expo-apple-authentication';
import { supabase } from '~/lib/supabase';

// Mock dependencies using bun

mock.module('expo-apple-authentication', () => ({
  signInAsync: mock(),
  isAvailableAsync: mock(),
  AppleAuthenticationScope: {
    FULL_NAME: 'fullName',
    EMAIL: 'email'
  },
  AppleAuthenticationButtonType: {
    SIGN_IN: 'signIn'
  },
  AppleAuthenticationButtonStyle: {
    BLACK: 'black'
  }
}));

mock.module('~/lib/supabase', () => ({
  supabase: {
    auth: {
      signInWithIdToken: mock()
    }
  }
}));

describe('Apple Authentication', () => {
  const mockCredential = {
    identityToken: 'mock-identity-token-12345',
    user: 'mock-user-id',
    email: 'test@privaterelay.appleid.com',
    fullName: {
      givenName: 'John',
      familyName: 'Doe'
    }
  };

  beforeEach(() => {
    mock.restore();
  });

  describe('signInWithApple', () => {
    it('should successfully authenticate with Apple', async () => {
      // Arrange
      const mockSignInAsync = mock(() => Promise.resolve(mockCredential));
      const mockSupabaseAuth = {
        signInWithIdToken: mock(() => Promise.resolve({
          data: { user: { id: 'user-123', email: 'test@privaterelay.appleid.com' } },
          error: null
        }))
      };
      
      AppleAuthentication.signInAsync = mockSignInAsync;
      (supabase.auth as any) = mockSupabaseAuth;

      // Act
      const result = await signInWithApple();

      // Assert
      expect(mockSignInAsync).toHaveBeenCalledWith({
        requestedScopes: [
          AppleAuthentication.AppleAuthenticationScope.FULL_NAME,
          AppleAuthentication.AppleAuthenticationScope.EMAIL,
        ],
      });
      
      expect(mockSupabaseAuth.signInWithIdToken).toHaveBeenCalledWith({
        provider: 'apple',
        token: 'mock-identity-token-12345',
      });
      
      expect(result.user.email).toBe('test@privaterelay.appleid.com');
    });

    it('should handle Apple authentication cancellation', async () => {
      // Arrange
      const mockSignInAsync = mock(() => Promise.reject(new Error('User cancelled Apple Sign In')));
      AppleAuthentication.signInAsync = mockSignInAsync;

      // Act & Assert
      await expect(signInWithApple()).rejects.toThrow('User cancelled Apple Sign In');
    });

    it('should handle Supabase authentication errors', async () => {
      // Arrange
      const mockSignInAsync = jest.mocked(AppleAuthentication.signInAsync);
      const mockSupabaseAuth = {
        signInWithIdToken: jest.fn().mockResolvedValue({
          data: null,
          error: { message: 'Invalid token' }
        })
      };
      
      mockSignInAsync.mockResolvedValue(mockCredential);
      (supabase.auth as any) = mockSupabaseAuth;

      // Act & Assert
      await expect(signInWithApple()).rejects.toThrow('Invalid token');
    });

    it('should handle private email relay', async () => {
      // Arrange
      const privateEmailCredential = {
        ...mockCredential,
        email: 'xyz123@privaterelay.appleid.com'
      };
      
      const mockSignInAsync = jest.mocked(AppleAuthentication.signInAsync);
      const mockSupabaseAuth = {
        signInWithIdToken: jest.fn().mockResolvedValue({
          data: { user: { id: 'user-123', email: 'xyz123@privaterelay.appleid.com' } },
          error: null
        })
      };
      
      mockSignInAsync.mockResolvedValue(privateEmailCredential);
      (supabase.auth as any) = mockSupabaseAuth;

      // Act
      const result = await signInWithApple();

      // Assert
      expect(result.user.email).toBe('xyz123@privaterelay.appleid.com');
    });
  });
});
```

### 1.2 Google Integration Service Unit Tests

**File**: `tests/unit/services/googleIntegration.test.ts`

```typescript
import { GoogleIntegrationService } from '~/services/googleIntegration';
import { getMyOAuthTokens } from '~/lib/auth/tokenStorage';
import { signInWithGoogle } from '~/lib/auth/googleAuth';

jest.mock('~/lib/auth/tokenStorage');
jest.mock('~/lib/auth/googleAuth');

describe('GoogleIntegrationService', () => {
  let service: GoogleIntegrationService;
  const mockUserId = 'user-123';

  beforeEach(() => {
    service = new GoogleIntegrationService();
    jest.clearAllMocks();
  });

  describe('checkGoogleConnection', () => {
    it('should return true when Google tokens exist', async () => {
      // Arrange
      const mockTokens = [
        {
          provider: 'google',
          access_token: 'mock-access-token',
          refresh_token: 'mock-refresh-token'
        }
      ];
      
      jest.mocked(getMyOAuthTokens).mockResolvedValue(mockTokens);

      // Act
      const result = await service.checkGoogleConnection(mockUserId);

      // Assert
      expect(result).toBe(true);
      expect(getMyOAuthTokens).toHaveBeenCalledWith('google');
    });

    it('should return false when no Google tokens exist', async () => {
      // Arrange
      jest.mocked(getMyOAuthTokens).mockResolvedValue([]);

      // Act
      const result = await service.checkGoogleConnection(mockUserId);

      // Assert
      expect(result).toBe(false);
    });

    it('should handle token retrieval errors', async () => {
      // Arrange
      jest.mocked(getMyOAuthTokens).mockRejectedValue(new Error('Database error'));

      // Act & Assert
      await expect(service.checkGoogleConnection(mockUserId)).rejects.toThrow('Database error');
    });
  });

  describe('initiateGoogleOAuth', () => {
    it('should successfully initiate Google OAuth', async () => {
      // Arrange
      const mockAuthResult = {
        user: { id: mockUserId, email: 'test@gmail.com' },
        session: { access_token: 'new-token' }
      };
      
      jest.mocked(signInWithGoogle).mockResolvedValue(mockAuthResult);

      // Act
      const result = await service.initiateGoogleOAuth();

      // Assert
      expect(result).toBe(true);
      expect(signInWithGoogle).toHaveBeenCalled();
    });

    it('should handle Google OAuth failures', async () => {
      // Arrange
      jest.mocked(signInWithGoogle).mockRejectedValue(new Error('OAuth cancelled'));

      // Act
      const result = await service.initiateGoogleOAuth();

      // Assert
      expect(result).toBe(false);
    });
  });
});
