      await detoxExpect(element(by.text('Welcome'))).toBeVisible();
      
      // Step 5: Try AI feature that requires Google
      await element(by.id('ai-feature-button')).tap();
      
      // Step 6: Should see Google integration prompt
      await detoxExpect(element(by.text('Connect Google Workspace'))).toBeVisible();
      await detoxExpect(element(by.text('Required for AI features'))).toBeVisible();
      
      // Step 7: User connects Google
      await element(by.text('Connect Google')).tap();
      
