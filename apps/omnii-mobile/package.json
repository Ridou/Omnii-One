{
  "name": "@omnii/omnii-mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "start": "EXPO_NO_TELEMETRY=1 expo start --dev-client",
    "dev": "EXPO_NO_TELEMETRY=1 expo start --dev-client",
    "dev:local": "EXPO_NO_TELEMETRY=1 expo start --dev-client --clear",
    "dev:tunnel": "EXPO_NO_TELEMETRY=1 expo start --dev-client --tunnel --clear",
    "dev:web": "EXPO_NO_TELEMETRY=1 expo start --web",
    "dev:ios": "EXPO_NO_TELEMETRY=1 expo start --dev-client --ios",
    "dev:android": "EXPO_NO_TELEMETRY=1 expo start --dev-client --android",
    "build:web": "expo export --platform web",
    "build:web:static": "expo export --platform web --output-dir dist-static",
    "build:web:spa": "expo export --platform web --output-dir dist-spa",
    "lint": "expo lint",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "test:oauth": "node test-oauth-tokens.js",
    "generate:assets": "cd scripts && node generate-assets.js",
    "generate:assets:clean": "cd scripts && node generate-assets.js --clean",
    "generate:ios": "cd scripts && node generate-assets.js --ios",
    "generate:android": "cd scripts && node generate-assets.js --android",
    "generate:web": "cd scripts && node generate-assets.js --web",
    "generate:help": "cd scripts && node generate-assets.js --help",
    "screenshots:resize": "node scripts/resize-screenshots.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@omnii/tailwind-config": "workspace:^",
    "@omnii/validators": "workspace:^",
