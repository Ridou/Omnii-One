    "events": "^3.3.0",
    "expo": "53.0.10",
    "expo-apple-authentication": "~7.2.4",
    "expo-auth-session": "~6.2.0",
    "expo-constants": "17.1.6",
    "expo-crypto": "~14.1.4",
    "expo-dev-client": "5.2.0",
    "expo-font": "~13.3.1",
