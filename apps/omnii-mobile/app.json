{
  "expo": {
    "name": "OMNII",
    "slug": "omnii",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icons/ios/app-icons/AppIcon-1024x1024@1x.png",
    "scheme": "omnii-mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "platforms": ["ios", "android", "web"],
    "jsEngine": "jsc",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.omnii.mobile",
      "googleServicesFile": "./ios/Omnii/GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": true,
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "omnii-mobile",
            "CFBundleURLSchemes": [
              "omnii-mobile"
            ]
          },
          {
            "CFBundleURLName": "google-oauth",
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.904371950268-abund19lqsma5d4rhfkkv212e5j7hv5e"
            ]
          }
        ]
      },
      "icon": "./assets/images/icons/ios/app-icons/AppIcon-1024x1024@1x.png"
    },
    "android": {
      "package": "com.omnii.mobile",
      "icon": "./assets/images/icons/android/app-icons/android-512x512.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/android/adaptive-icons/adaptive-foreground-108x108.png",
        "backgroundColor": "#007AFF"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./public/favicon.svg"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    },
    "owner": "omnii",
    "splash": {
      "image": "./assets/images/icons/ios/app-icons/AppIcon-1024x1024@1x.png",
      "backgroundColor": "#ffffff",
      "resizeMode": "contain"
    }
  }
}