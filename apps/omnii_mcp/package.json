    "@langchain/core": "^1.1.17",
    "@langchain/textsplitters": "^1.0.1",
    "@modelcontextprotocol/sdk": "^1.12.0",
    "@omnii/api": "workspace:*",
    "@omnii/validators": "workspace:^",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.211.0",
    "@opentelemetry/sdk-node": "^0.211.0",
