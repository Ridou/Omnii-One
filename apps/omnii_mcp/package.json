    "express-rate-limit": "^7.5.0",
    "file-type": "^21.3.0",
    "follow-redirects": "^1.15.9",
    "google-auth-library": "^9.15.1",
    "googleapis": "^149.0.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.6.1",
    "mammoth": "^1.11.0",
    "markdown-it": "^14.1.0",
    "morgan": "^1.10.0",
    "neo4j-driver": "^5.9.0",
    "openai": "^4.67.1",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "redis": "^4.6.10",
    "twilio": "^5.6.1",
    "unpdf": "^1.4.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.43"
  },
  "devDependencies": {
    "@types/bun": "^1.2.15",
    "@types/cors": "^2.8.14",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.5",
    "@types/node": "^22.15.29",
    "@types/redis": "^4.0.11",
    "bun-types": "latest",
    "eventsource": "^4.0.0",
    "jest": "^29.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.8.3"
  }
}
